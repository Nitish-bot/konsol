{
  "name": "konsol",
  "scripts": {
    "android": "expo run:android",
    "android:build": "expo prebuild -p android",
    "build": "tsc --noEmit && npm run android:build",
    "ci": "tsc --noEmit && npm run lint:check && npm run fmt:check && npm run android:build",
    "dev": "expo start",
    "doctor": "npx -y expo-doctor@latest",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "expo lint --fix",
    "lint:check": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "start": "expo start",
    "web": "expo start --web",
    "clean": "cd android && ./gradlew clean"
  },
  "description": "A template for building a Solana mobile app with Expo and React Native Paper.",
  "keywords": [
    "expo",
    "mobile-wallet-adapter",
    "react-native",
    "react-native-paper",
    "solana-web3js"
  ],
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-snackbar"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "main": "./index.js",
  "version": "1.0.0",
  "dependencies": {
    "@ethersproject/shims": "^5.8.0",
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.2",
    "@privy-io/expo": "^0.59.6",
    "@privy-io/expo-native-extensions": "^0.0.5",
    "@privy-io/js-sdk-core": "^0.56.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.2.4",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.2.4",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "@tanstack/react-query": "^5.85.5",
    "@wallet-standard/core": "^1.1.1",
    "buffer": "^6.0.3",
    "deepmerge": "^4.3.1",
    "expo": "54.0.15",
    "expo-apple-authentication": "~8.0.7",
    "expo-application": "~7.0.7",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.15",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "fast-text-encoding": "^1.0.6",
    "js-base64": "^3.7.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
    "react-native-passkeys": "^0.3.3",
    "react-native-qrcode-styled": "^0.4.0",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.1",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "react-qr-code": "^2.0.16",
    "viem": "^2.38.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@types/react": "~19.1.17",
    "eslint": "^9.33.0",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "private": true
}
